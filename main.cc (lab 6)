#include <iostream>
#include <map>
#include <list>
#include "selectionsort.h"

using namespace std;

int main()
{
    vector<int> test;
    
    test.push_back(2);
    test.push_back(4);
    test.push_back(5);
    test.push_back(1);
    test.push_back(8);
    test.push_back(9);
    
    selectionsort(test); // sort list
    
    // map<int, int> test2;
    
    // test2.insert(pair<int, int>(1, 2));
    // test2.insert(pair<int, int>(3, -1));
    // test2.insert(pair<int, int>(-1, 3));
    
    // selectionsort(test2);
    
    list<int> test3;
    
    test3.push_back(10);
    test3.push_back(1);
    test3.push_back(99);
    test3.push_back(15);
    test3.push_back(15);
    test3.push_back(4);
    
    selectionsort(test3); // sort list
    
    
    // test with empty vector and list
    
    vector<int> empty;
    
    selectionsort(empty);
    
    list<int> empty2;
    
    selectionsort(empty2);
    
    
    // vector< pair<int, int> > test4;
    
    // selectionsort(test4);
    
    return 0;
}
